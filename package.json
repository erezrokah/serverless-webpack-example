{
  "name": "serverless-webpack-example",
  "description": "serverless webpack example",
  "private": true,
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "deploy": "sls deploy",
    "remove": "sls remove",
    "lint": "eslint src/**/*.js __tests__/**/*.js",
    "prettier": "prettier --write src/**/*.js __tests__/**/*.js",
    "prettier:ci": "prettier --list-different src/**/*.js  __tests__/**/*.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.0",
    "jest": "^26.0.1",
    "jest-e2e-serverless": "^0.0.21",
    "prettier": "^2.0.5",
    "serverless-webpack": "^5.3.2",
    "webpack": "^4.43.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./node_modules/jest-e2e-serverless/lib/index.js"
    ],
    "testPathIgnorePatterns": [
      "config.js"
    ]
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "source-map-support": "^0.5.19"
  }
}
